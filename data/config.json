{
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "static_dir": "web",
    "max_servers": 10,
    "default_memory": 2048
  },
  "java": {
    "auto_detect": true,
    "java_path": "",
    "default_args": [
      "-Xms1G",
      "-Xmx2G",
      "-XX:+UseG1GC"
    ],
    "min_version": 8,
    "max_memory": 8192
  },
  "download": {
    "cores_dir": "data/cores",
    "timeout": 1800000000000,
    "max_retries": 3,
    "chunk_size": 1048576,
    "fastmirror_api": "https://download.fastmirror.net/api/v3"
  },
  "logging": {
    "level": "info",
    "logs_dir": "data/logs",
    "max_file_size": 10485760,
    "max_files": 5,
    "max_age": 7,
    "enable_colors": true
  },
  "daemon": {
    "enable_auto_restart": true,
    "max_restart_attempts": 3,
    "restart_delay": 5000000000,
    "crash_detection_delay": 3000000000,
    "resource_monitoring": true,
    "monitor_interval": 30000000000,
    "log_rotation": true,
    "max_log_size": 52428800,
    "max_log_files": 10
  },
  "frp": {
    "enabled": false,
    "api_endpoint": "https://api.openfrp.net",
    "default_node": "",
    "auto_start": false,
    "auto_restart": true,
    "max_tunnels": 10,
    "monitor_interval": 30000000000,
    "stats_retention": 86400000000000,
    "default_bandwidth": 1024,
    "default_compression": true,
    "default_encryption": false
  }
}